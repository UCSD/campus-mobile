    "react-native-simple-toast": "0.0.8",
    "react-native-sortable-list": "0.0.21",
    "react-native-svg": "6.5.2",
    "react-native-vector-icons": "4.5.0",
    "react-navigation": "1.5.8",
    "react-navigation-header-buttons": "0.0.5",
    "react-redux": "5.0.7",
